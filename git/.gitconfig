[user]
	name = Nolan Lancaster
	email = nolancaster@gmail.com
[core]
	editor = vim
	excludesfile = ~/.gitignore_global
	autocrlf = input
[alias]
	co = checkout
	br = branch
	com = commit
	st = status
[push]
	default = simple
[credential]
	helper = cache --timeout=3600
[branch]
	autosetuprebase = always
[rerere]
	enabled = true
